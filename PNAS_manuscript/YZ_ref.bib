@inbook{Bucher2019a,
	abstract = {Abstract Nuclear magnetic resonance spectroscopy is widely used in chemical analysis but is typically limited to large sample volumes (>microliter) due to its inherent low sensitivity. In recent years, a new magnetic field sensor, based on nitrogen-vacancy (NV) defects in the diamond lattice, has shown to overcome this limitation and it has been demonstrated that NMR signals from nanoscale volumes and even from single molecules can be detected. In this article, we introduce the basic principles of NMR spectroscopy with NV-centers in diamond for nano- and microscale sample volumes. In the first part, we describe the properties of the NV-center, which form the basis for its quantum sensing capabilities. In the second part, we discuss in detail the measurement schemes for detecting magnetic fields and NMR signals. In the third part, the origin of nanoscale and macroscopic NMR signals are described and the consequence for the corresponding NMR linewidth will be analyzed. In the last part, we summarize recent NV-NMR experiments and we give an outlook for this rapidly developing technology.},
	author = {Bucher, Dominik B.},
	booktitle = {eMagRes},
	date-added = {2021-08-18 13:53:35 -0700},
