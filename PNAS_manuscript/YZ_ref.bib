	isbn = {9780470034590},
	keywords = {NV-diamond, nitrogen vacancy center, quantum sensing, microscale NMR, nanoscale NMR},
	pages = {363-370},
	publisher = {American Cancer Society},
	title = {Principles of Nano- and Microscale NMR-Spectroscopy with NV-Diamond Sensors},
