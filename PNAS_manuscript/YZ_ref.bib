@inbook{Berthelin1991a,
	abstract = {The weathering of soil minerals in the plant rhizosphere was studied. Results obtained by utilizing experimental devices to study the interactions between 'roots-mycorrhizal fungi-bacteria and soil minerals', in the rhizosphere of graminaceous plants (maize and rice) and of trees (pine) are presented. Plant roots alone (axenic roots) can take up and solubilize mineral elements (P, K etc.) from rock phosphates and phyllosilicates (biotite, phlogopite). Rhizospheric microflora (endo- or ectomycorrhiza and rhizobacteria) are able to modify these processes. Such interactions between roots-microorganisms and minerals, that depend on the plant growth conditions, promote the transformation of minerals such as the mica phlogopite to the clay vermiculite. In experimental conditions, bacteria appear mainly as acid-complexing producing agents and mycorrhiza as agents increasing the exchange surface.},
	address = {Oxford},
	annote = {Author Affiliation: Centre de P{\'e}dologie Biologique, CNRS 17, rue Notre-Dame-des Pauvres B.P.5, 54501 Vandoeuvre-les-Nancy C{\'e}dex, France.},
