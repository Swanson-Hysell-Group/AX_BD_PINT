	journal = {The Journal of Geology},
	number = {5},
	pages = {630-638},
	title = {The Carthage-Colton Mylonite Zone (Adirondack Mountains, New York): The Site of a Cryptic Suture in the Grenville Orogen?},
	url = {https://doi.org/10.1086/629613},
